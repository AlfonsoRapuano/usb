<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record model="ir.rule" id="task_assign_to_multi_user_rule">
            <field name="name">task Multi User Assign</field>
            <field name="model_id" search="[('model','=','project.task')]" model="ir.model"/>
            <field name="domain_force">[('assign_user_ids', 'in', [user.id])]</field>
            <field eval="[(4,ref('base.group_user'))]" name="groups"/>
        </record>

    </data>
</odoo>
